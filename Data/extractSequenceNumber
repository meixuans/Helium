import csv
import numpy as np

def extract_sequence_number_from_CSV(fileName):
    filePath = "./%s.csv" % (fileName)
    sequenceNumberList = []

    with open(filePath, newline='') as csvfile:
        data = csv.reader(csvfile, delimiter=',')

        for row in data:
            if row[5]=="motion":
                sequenceNumberList.append(row[7])

    return sequenceNumberList


def extract_sequence_number_time_pair_from_CSV(fileName):
    filePath = "./%s.csv" % (fileName)
    sequenceNumberWithTimeList = []

    with open(filePath, newline='') as csvfile:
        data = csv.reader(csvfile, delimiter=',')

        for row in data:
            if row[5]=="motion":
                tuple = (row[0], row[7])
                if sequenceNumberWithTimeList[-1][-1] == row[7]:
                    continue
                sequenceNumberWithTimeList.append(tuple)

    return sequenceNumberWithTimeList

if __name__ == '__main__':
    fileNames = ["1434-1543"]
    for fileName in fileNames:
        sequenceNumberList = extract_sequence_number_from_CSV(fileName)
